            			<xsd:simpleType>
            				<xsd:restriction base="xsd:string">
            					<xsd:pattern value="(\d{1,8})?"></xsd:pattern>
            				</xsd:restriction>
            			</xsd:simpleType>
            		</xsd:element>
            		<xsd:element name="BEGDA" type="xsd:date">
            			<xsd:annotation>
            				<xsd:documentation>
            					Начальная дата
            				</xsd:documentation>
            			</xsd:annotation>
            		</xsd:element>
            		<xsd:element name="ENDDA" type="xsd:date">
            			<xsd:annotation>
            				<xsd:documentation>
            					Конечная дата
            				</xsd:documentation>
            			</xsd:annotation>
            		</xsd:element>

            		<xsd:element name="SHORT">
            			<xsd:annotation>
            				<xsd:documentation>
            					Код объекта
            				</xsd:documentation>
            			</xsd:annotation>
            			<xsd:simpleType>
            				<xsd:restriction base="xsd:string">
            					<xsd:maxLength value="12"></xsd:maxLength>
            				</xsd:restriction>
            			</xsd:simpleType>
            		</xsd:element>
            		<xsd:element name="STEXT">
                        <xsd:annotation>
                        	<xsd:documentation>Название объекта</xsd:documentation></xsd:annotation>
                        <xsd:simpleType>
            				<xsd:restriction base="xsd:string">
            					<xsd:maxLength value="40"></xsd:maxLength>
            				</xsd:restriction>
            			</xsd:simpleType>
            		</xsd:element>
            	</xsd:sequence>
            </xsd:complexType>
            		
        
            <xsd:complexType name="RESPONSEType">
            	<xsd:annotation>
            		<xsd:documentation>
            			Результат выполнения
            		</xsd:documentation>
            	</xsd:annotation>
            	<xsd:sequence>
            		<xsd:element name="RETCODE">
            			<xsd:annotation>
            				<xsd:documentation>
            					Тип сообщения. Допустимые значения: 
            					S-успешно,  E-ошибка, 
            					W-предупреждение.
            				</xsd:documentation>
            			</xsd:annotation>
            			<xsd:simpleType>
            				<xsd:restriction base="xsd:string">
            					<xsd:maxLength value="1"></xsd:maxLength>
            				</xsd:restriction>
            			</xsd:simpleType>
            		</xsd:element>
            		<xsd:element name="MESSAGE">
                        <xsd:annotation>
                        	<xsd:documentation>Текст сообщения
E	0	По запрошенному коду предприятия ЦНСИ ОЕ не найдена
W	1	По запрошенному коду предприятия ЦНСИ отсутствуют нормы времени
S	2	Данные успешно переданы
W	3	По запрошенным параметрам данные отсутствуют
W	4	Изменения по запрошенным данным отсутствуют
W	5	По запрошенному коду предприятия ЦНСИ изменения отсутствуют
</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
            				<xsd:restriction base="xsd:string">
            					<xsd:maxLength value="220"></xsd:maxLength>
            				</xsd:restriction>
            			</xsd:simpleType>
            		</xsd:element>
            	</xsd:sequence>
            </xsd:complexType>
